   "source": [
    "N = int(input(\"Numbers:\"))\n",
    "a = 0\n",
    "b = 0\n",
    "c = 0\n",
    "for i in range(1,N+1):\n",
    "    if i % 2 != 0:\n",
    "        a+=i\n",
    "        c+=1\n",
    "    else:\n",
    "        b+=i\n",
    "        c+=1\n",
    "        \n",
    "print(\"Sum of odd numbers:\",a)\n",
    "print(\"Average of even numbers\",b/c)"
   ]
  }
